    .last::after{
        content: "";
        position: absolute;
        /* bottom: -5px; */
        height: 3px;
        width: 100%;
        left :0;
        bottom: 0;
        background-color: red;
        transition: 0.3s;
        transition-timing-function: ease-in-out;
        transform:scaleX(0) ;
        text-decoration: underline solid red;
    }
    .last:hover::after{
        transform:scaleX(1) ;
    }
